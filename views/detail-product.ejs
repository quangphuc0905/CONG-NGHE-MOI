			<div class="back-link">
				<a href="/"> &lt;&lt; Trở về trang chủ</a>
			</div>
			<div class="row">
				<div class="col-lg-4">
					<div style="text-align: center; max-width: 70%;">
						<img id="hinhanh" class="product-big-img" src="images/1.jpg" alt="">
					</div>
				</div>
				<div class="col-lg-8 product-details">
					<h2 id="tensanpham" class="p-title"></h2>
					<h3 id="giasanpham" class="p-price">$39.90</h3>
					
					<div class="quantity">
						<p>Quantity</p>
                        <div class="pro-qty"><input id="soluongsp" type="text" value="1"></div>
					</div>
					<%var dem = uname%>
					<%var demm = uname%>
					<%if(!dem){%>
						<a id="muahang" class="site-btn" data-toggle="modal" data-target="#myModal">MUA NGAY</a>
					<%}%>
					<%if(demm){%>
						<a href="/checkout" id="muahang" class="site-btn">MUA NGAY</a>
					<%}%>

					<div id="accordion" class="accordion-area">
						<div class="panel">
							<div class="panel-header" id="headingOne">
								<button class="panel-link active" data-toggle="collapse" data-target="#collapse1" aria-expanded="true" aria-controls="collapse1">Mô tả:</button>
							</div>
							<div id="collapse1" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
								<div class="panel-body">
									<p id="des"></p>
								</div>
							</div>
						</div>
						
					</div>
					
				</div>
			</div>
		</div>
	</section>
	<!-- product section end -->

