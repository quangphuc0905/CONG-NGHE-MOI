	<script>
		//show quantity in the cart
			var a =JSON.parse(sessionStorage.getItem('shoppingCart'))
			var k=0;
			console.log(a)
			if(a!=null)
			{
				for(var i=0; i<a.length; i++)
				{
					k += a[i].count;
				}
			}
			
			document.getElementById('quantity').innerHTML = k;
		//show detail product
		
		var b =JSON.parse(sessionStorage.getItem('detailProduct'))
		var dem = b.length;
		document.getElementById('tensanpham').innerHTML = b[dem-1].name;
		document.getElementById('giasanpham').innerHTML = b[dem-1].price+ " VND";
		document.getElementById('soluongsp').innerHTML = b[dem-1].count;
		document.getElementById('hinhanh').src = b[dem-1].images;
		
		
		arrProduct = [];
				var id = b[dem-1].id;
				var name = b[dem-1].name;
				var price = b[dem-1].price;
				var count = b[dem-1].count;
				var images = b[dem-1].images;
				var ids = b[dem-1].ids;
				var names = b[dem-1].names
		
		function Item( id, name, price, count, images, ids, names) {
		this.id = id;
		this.name = name;
		this.price = price;
		this.count = count;
		this.images = images;
		this.ids = ids;
		this.names = names;
	  }
	  var item = new Item(id, name, price, count, images, ids, names);
	  console.log(item);
	arrProduct.push(item);
	
	</script>
